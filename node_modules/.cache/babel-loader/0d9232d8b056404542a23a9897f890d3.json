token)\r\n        var url2 = \"http://localhost:8000/api-token-auth/\"\r\n\r\n            fetch(url2,{\r\n                method: 'POST',\r\n                headers: {\r\n                    'Accept': 'application/json',\r\n                    'Content-Type': 'application/json',\r\n                    'X-CSRFToken': csrftoken\r\n                  }, \r\n                  credentials: 'include',\r\n                  body: inputsForm\r\n              }).then(response => {\r\n            if(response.ok) {\r\n                console.log(\"fetchLogin\",response)\r\n                return response.json();\r\n            } else {\r\n                console.log(response,'Mauvaise réponse du réseau');\r\n            }\r\n            }).then(data => \r\n                {\r\n                    console.log(\"reponse\",data)\r\n                }\r\n                )\r\n            .catch(function(error) {\r\n            console.log('Il y a eu un problème avec l\\'opération fetch: ' + error.message);\r\n            });\r\n    }\r\n    return (\r\n        <div className=\"d-flex flex-column align-items-center justify-content-center p-3\">\r\n            {logo}\r\n            <form action=\"\" className=\"formclassName\">\r\n                <a className=\"googleBtn d-flex align-items-center mb-4\">\r\n                    <img src={Google} alt=\"logo google\" className=\"iconS\"/>\r\n                        Continuer avec Google\r\n                </a>\r\n                <div className=\"formrow\">\r\n                    <input id=\"in1\" type=\"email\" name='username' className=\"inputform\" placeholder=\" \" required/>\r\n                        <label for=\"in1\" className=\"labelform\">Email*</label>\r\n                </div>\r\n                <div className=\"formrow \">\r\n                    <input id=\"in2\" type=\"password\" name='password' className=\"inputform \" placeholder=\" \" required/>\r\n                        <label for=\"in2\" className=\"labelform\">Mot de passe*</label>\r\n                </div>\r\n                <div className=\"w-100 d-flex justify-content-between align-items-center pb-3\">\r\n                    <div className=\"d-flex align-items-center \">\r\n                        <input type=\"checkbox\" className=\"formCheckbox me-2\"/>\r\n                            <label for=\"stayco\" className=\"m-0\">\r\n                                Rester connecté\r\n                            </label>\r\n                    </div>\r\n                    <a href=\"\" className=\"link ms-2\">Mot de passe oublié</a>\r\n                </div>\r\n                <button className=\"formbtn\" onClick={(e) => connexion(e)}>\r\n                    Se connecter\r\n                </button>\r\n                <div className=\"traitHorizontal\">\r\n                    <span className=\"textTrait\">\r\n                        ou\r\n                    </span>\r\n                </div>\r\n                <a className=\"googleBtn\" onClick={() => props.setModalcontent(<SignupForm setModalcontent={fs}/>)}>\r\n                    S'inscrire\r\n                </a>\r\n            </form>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default Loginform;\r\n"]},"metadata":{},"sourceType":"module"}